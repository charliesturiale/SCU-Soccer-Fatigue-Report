{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "store:default",
    "store:allow-load",
    "store:allow-save",
    "store:allow-get",
    "store:allow-set",
    "core:path:allow-resolve-directory",
    "core:path:default",
    "core:default",
    "shell:allow-execute"
  ]
}